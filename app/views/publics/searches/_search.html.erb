<% if customer_signed_in? %>
  <div>
    <%= form_with url: search_path do |f| %>
      <%= f.text_field :word %>
      <%= f.select :range, option_for_select([["name"], ["address"]]) %>
      <%= f.select :search, options_for_select([["完全一致","perfect_match"], ["部分一致","partial_match"]]) %>
      <%= f.submit "検索", class:"btn btn-info" %>
    <% end %>
  </div>
<% end %>