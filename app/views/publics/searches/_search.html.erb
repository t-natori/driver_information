<% if customer_signed_in? %>
  <div>
    <%= form_with url: search_path, method: :get do |f| %>
      <%= f.text_field :word, class:"rounded", placeholder: "住所検索" %>
      <%#= f.select :range, options_for_select([["post"], ["customer"]]) %>
      <%= f.select :search, options_for_select([["部分一致","partial_match"], ["完全一致","perfect_match"]]) %>
      <%= f.submit "検索", class:"rounded" %>
    <% end %>
  </div>
<% end %>